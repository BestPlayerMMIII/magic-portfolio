{
  "name": "magic-portfolio",
  "version": "1.0.0",
  "description": "Interactive wizard laboratory developer portfolio",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"SERVER,CLIENT\" --prefix-colors \"red,blue\" --kill-others-on-fail --restart-tries 3 \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "vite --host",
    "build": "vite build && npm run build:server",
    "build:server": "cd server && npm run build",
    "preview": "vite preview",
    "start": "npm run start:server",
    "start:server": "cd server && npm start",
    "postinstall": "cd server && npm install"
  },
  "keywords": [
    "portfolio",
    "vue",
    "threejs",
    "typescript",
    "node"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "@types/three": "^0.157.0",
    "pinia": "^3.0.3",
    "three": "^0.157.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  }
}
